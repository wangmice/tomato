## apcupsd.conf v1.1 ##
UPSCABLE @UPSCABLE@
UPSTYPE @UPSTYPE@
DEVICE @SERIALDEV@
LOCKFILE @LOCKDIR@
SCRIPTDIR @sysconfdir@
PWRFAILDIR @PWRFAILDIR@
NOLOGINDIR @nologdir@
ONBATTERYDELAY 6
BATTERYLEVEL 5
MINUTES 3
TIMEOUT 0
ANNOY 300
ANNOYDELAY 60
NOLOGON disable
KILLDELAY 0
NETSERVER on
NISIP @NISIP@
NISPORT @NISPORT@
EVENTSFILE @LOGDIR@/apcupsd.events
EVENTSFILEMAX 10
UPSCLASS standalone
UPSMODE disable
STATTIME 0
STATFILE @LOGDIR@/apcupsd.status
LOGSTATS off
DATATIME 0
